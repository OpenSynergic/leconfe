{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akaunting/laravel-setting": "^1.2",
        "awcodes/filament-tiptap-editor": "^3.0@beta",
        "awcodes/shout": "^2.0",
        "binarytorch/larecipe": "^2.6",
        "codeat3/blade-iconpark": "^1.5",
        "coolsam/flatpickr": "^3.0",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "filament/spatie-laravel-tags-plugin": "^3.0",
        "gehrisandro/tailwind-merge-laravel": "^0.2.1",
        "genealabs/laravel-model-caching": "^0.13.4",
        "guzzlehttp/guzzle": "^7.2",
        "kra8/laravel-snowflake": "^2.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "lorisleiva/laravel-actions": "^2.5",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.0",
        "plank/laravel-metable": "^5.4",
        "rahmanramsi/livewire-page-group": "^0.0.4",
        "rickselby/laravel-gate-cache": "^3.6",
        "ryangjchandler/filament-navigation": "^1.0@beta",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/laravel-tags": "^4.3",
        "squirephp/countries-en": "^3.4",
        "stechstudio/filament-impersonate": "^3.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "pint",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "@php artisan icons:cache"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
